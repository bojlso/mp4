<Window x:Class="cpu_gpu.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Window2" Height="400" Width="150">
    <Grid Margin="5,5,5,5">

        <ListBox Margin="10,0,10,0">
            <TextBox x:Name="cpu_no"  Margin="3,3,3,3" />
            <TextBox x:Name="cpu_idle" Margin="3,3,3,3" />
            <TextBox x:Name="cpu_full" Margin="3,3,3,3"/>
            <TextBox x:Name="cpu_cores" Margin="3,3,3,3"/>
            <TextBox x:Name="cpu_threads" Margin="3,3,3,3"/>
            <TextBox x:Name="cpu_l2" Margin="3,3,3,3"/>
            <TextBox x:Name="cpu_l3" Margin="3,3,3,3"/>
            <TextBox x:Name="cpu_name" Margin="3,3,3,3"/>
            <TextBox x:Name="cpu_tdp" Margin="3,3,3,3"/>
            <TextBox x:Name="cpu_price" Margin="3,3,3,3"/>
            <TextBox x:Name="cpu_memory_type" Margin="3,3,3,3"/>
            <TextBox x:Name="cpu_score" Margin="3,3,3,3"/>
        </ListBox>
    </Grid>
</Window>
